CREATE DATABASE STUDENT_MANAGEMENT;
USE STUDENT_MANAGEMENT;	

CREATE TABLE STUDENT(
STUDENT_ID INT ,
FIRST_NAME VARCHAR(255),
LAST_NAME VARCHAR(255),
GENDER VARCHAR(255),
AGE INT,
ADDRESS VARCHAR(255),
MOBLIE INT,
primary key (STUDENT_ID));

SELECT * FROM STUDENT;

